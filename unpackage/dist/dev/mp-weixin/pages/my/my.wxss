.areo-main{
	letter-spacing: 2rpx;
    background-color: #041746;
}
.areo-top{
    display: block;
    padding-top: 30rpx;
    padding-left: 48rpx;
    padding-right: 48rpx;
    color: #fff;
}
.user0{
    display: block;
    padding-bottom: 90rpx;
}
.platform{
	float: left;
}
.tipRegistrate{
	position: fixed;
	bottom: 100rpx; /* differents */
	height: 100rpx;
	width: 100%;
	background-color: #041746;
	z-index: 99;
    color: #fff;
}
.tipRegistrate-text{
	font-size: 28rpx;
	line-height: 50rpx;
	display: block;
	text-align: center;
}
.tipDetail{
	display: inline-block;
}
.toRegistrate{
	margin-left: 50rpx;
	margin-top: 25rpx;
	margin-bottom: 25rpx;
	width: 150rpx;
	height: 50rpx;
	background-color: #0281FF;
	display: inline-block;
	border-radius: 10rpx;
}
.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  overflow: hidden;
  background-color: #f0f0f0;
  z-index: 90;
}
.arrivalNavigation {
	width: 250rpx;
	position: absolute;
	left: 20rpx;
	top: 80rpx;
	z-index: 99;
}
.arrivalNavigation .sideNavigation {
	width: 248rpx;
	background-color: #e8f0ff;
    -webkit-filter: opacity(90%);
            filter: opacity(90%);
	color: #eee;
	border-radius: 10rpx;
}
.sideNavigation .liBottomBorder {
	border: 0.1rpx solid #b6b6b6;
}
.sideNavigation .navigatorStyle {
	height: 85rpx;
	text-align: center;
	line-height: 85rpx;
	font-size: 28rpx;
	color: #0281ff;
}
.address{
	float: right;
	font-size: 32rpx;
}
.user1{
    display: flex;
}
.user-image{
    margin-right: 26rpx;
    width: 180rpx;
    height: 180rpx;
    border-radius: 24rpx;
}
.user1 .user-info{
    height: 170rpx;
	display: block;
}
.user1 .user-name{
    display: block;
	height: 42rpx;
    padding-left: 30rpx;
    font-size: 32rpx;
    font-weight: 500;
    line-height: 42rpx;
    padding-top: 10rpx;
}
.user1 .user-info .user-identity-block{
	display: flex;
	height: 42rpx;
	margin-left: 30rpx;
    margin-top: 25rpx;
}
.user1 .user-identity{
	margin-right: 30rpx;
    width: 80rpx;
    height: 42rpx;
    border-radius: 16rpx;
    background-color: #ffffff;
    -webkit-filter: opacity(80%);
            filter: opacity(80%);
	box-shadow: 0 0 10rpx #FFFFFF;
	
    z-index: 10;
    font-size: 26rpx;
    line-height: 42rpx;
    text-align: center;
	color: #006AD7;
}
.user1 .check-bar{
    display: inline-block;
    width: 18rpx;
    height: 18rpx;
    line-height: 42rpx;
    background-color: #FFA448;
    border-radius: 20rpx;
    margin-top: 13rpx;
    margin-right: 14rpx;
}
.user .check-text{
    color: #FFA448;
    font-size: 30rpx;
    line-height: 42rpx;
    text-align: center;
}
.user-follow{
    width: 250rpx;
    line-height: 28rpx;
    font-size: 20rpx;
	display: block;
    /* display: flex; */
    justify-content: flex-end;
    padding-left: 30rpx;
    padding-top: 25rpx;
}
.user-follow .followed{
    padding-right: 30rpx;
}
.user2{
    display: block;
    margin-top: 30rpx;
    margin-bottom: 50rpx;
}
.user2 .user-tags{
	display: flex;
	margin-top: 20rpx;
	height: 42rpx;
}
.tag-background{
	display: flex;
}
.user-tags .tag-item{
	border-radius: 16rpx;
	margin-left: 8rpx;
	margin-right: 8rpx;
	height: 40rpx;
	width: 130rpx;
    background-color: #ffffff;
    -webkit-filter: opacity(50%);
            filter: opacity(50%);
}
.user-tags .tag-text{
	z-index: 10;
	display: block;
	margin-left: 8rpx;
	margin-right: 8rpx;
	font-size: 24rpx;
	line-height: 40rpx;
	text-align: center;
	color: #006AD7;
	-webkit-filter: none;
	        filter: none;
}
.discounts{
	/* right: 60rpx; */
	margin-left: 35rpx;
	font-size: 32rpx;
}
.areo-body-box{
	letter-spacing: 2rpx;
    background-color: #fff;
    border-top-left-radius: 58rpx;
    border-top-right-radius: 58rpx;
}
.areo-body{
    padding-left: 48rpx;
    padding-right: 48rpx;
    padding-top: 82rpx;
}
.title{
    color: #1E6CB5;
    font-size: 28rpx;
    line-height: 30rpx;
    margin-bottom: 20rpx;
    /* font-family: "PingFang SC"; */
}
.title .title-bar{
    display: inline-block;
    width: 10rpx;
    height: 24rpx;
    background-color: #21277B;
    border-radius: 4rpx;
    margin-right: 14rpx;
}
.title-name{
    letter-spacing: 6rpx;
}
.more-history{
	float: right;
    font-size: 22rpx;
	margin-right: 80rpx;
	color: #51555a;
}
.schedule-and-order{
    margin-bottom: 48rpx;
    margin-left: 24rpx;
    background-color: #ebf3fc;
    border-radius: 10rpx;
    padding: 8rpx 0 8rpx 0;
    text-align: center;
}
.schedule-and-order text{
    color: #4f73a5;
    line-height: 30rpx;
    font-size: 22rpx;
}
.collection-block{
    height: 82rpx;
    display: flex;
    margin-bottom: 44rpx;
}
.collection-block .collection-item{
    /* flex: 1; */
	width: 220rpx;
	padding: 14rpx 20rpx 14rpx 20rpx;
    /* display: inline-block; */
    /* justify-content: center; */
    /* align-items: center; */
    background-color: #ebf3fc;
    margin-left: 30rpx;
    border-radius: 24rpx;
}
.collection-block .collection-item .category{
	float: left;
    font-size: 24rpx;
    line-height: 32rpx;
    color: #4f73a5;
}
.collection-block .collection-item .collection-count{
	clear: left;
	float: left;
    font-size: 24rpx;
	-webkit-transform-origin: 0 0;
	        transform-origin: 0 0;
	-webkit-transform:scale(0.7);
	        transform:scale(0.7);
    line-height: 32rpx;
    color: #707070;
}
.areo-history-track{
    padding-left: 48rpx;
}
.history-block{
    display: block;
}
.history-item{
    display: inline-block;
    /* text-align: center; */
    margin-right: 22rpx;
	vertical-align: top;
}
.history-item .pic-box{
    /* height: 142rpx; */
	margin-top: 5rpx;
    height: 194rpx;
    width: 194rpx;
    display: flex;
	float: left;
    flex-flow: row-reverse;
}
.history-item image{
    width: 184rpx;
    height: 184rpx;
    border-radius: 20rpx;
    box-shadow: 0rpx 6rpx 6rpx #738eae, -6rpx 0 6rpx #738eae, 5rpx 0 5rpx #738eae;
}
.areo-bottom{
    margin:20rpx 0 40rpx 0;
    text-align: center;
    font-size: 22rpx;
    color: #006AD7;
	/* margin-bottom: 20rpx; */
}
