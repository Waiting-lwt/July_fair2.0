<view class="areo-main"><block wx:if="{{isLogin===false}}"><view class="tipRegistrate"><view class="modal-mask"></view><view class="tipDetail"><text class="tipRegistrate-text" style="padding-left:50rpx;">注册认证后即可解锁完整功能</text></view><view data-event-opts="{{[['tap',[['toLogin',['$event']]]]]}}" class="toRegistrate" bindtap="__e"><text class="tipRegistrate-text">注册认证</text></view></view></block><view class="areo-top"><view class="user0"><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="platform" bindtap="__e"><text class="icon-3lines-white"></text></view><block wx:if="{{showSpringBox==true}}"><view data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" class="modal-mask" bindtap="__e"></view></block><block wx:if="{{showSpringBox}}"><view class="arrivalNavigation"><view class="sideNavigation"><navigator class="navigatorStyle" url="../../pages/index/index"><text>平台规则</text></navigator><view class="liBottomBorder"></view><navigator class="navigatorStyle" url="../../pages/index/index"><text>联系平台</text></navigator></view></view></block><view class="address"><text class="icon-car-address"></text><text data-event-opts="{{[['tap',[['toAddress']]]]}}" style="margin-left:10rpx;" bindtap="__e">收货地址</text></view></view><view class="user1"><view data-event-opts="{{[['tap',[['toMyself']]]]}}" bindtap="__e"><image class="user-image" src="{{userMine.portrait}}" mode="aspectFill" lazy-load="false" data-event-opts="{{[['error',[['',['$event']]]],['load',[['',['$event']]]]]}}" binderror="__e" bindload="__e"></image></view><view class="user-info"><text class="user-name">{{userMine.name}}</text><view class="user-identity-block"><block wx:if="{{userMine.type==0||userMine.type==1}}"><block wx:if="{{userMine.type==0}}"><text class="user-identity">学生</text></block><block wx:if="{{userMine.type==1}}"><text class="user-identity">商家</text></block><text class="iconfont icon-hook-blue" style="margin-top:5rpx;"></text></block><block wx:if="{{userMine.type==2||userMine.type==3}}"><text class="check-bar"></text><block wx:if="{{userMine.type==2}}"><text class="check-text">审核未通过</text></block><block wx:if="{{userMine.type==3}}"><text class="check-text">未审核</text></block></block></view><text class="user-follow"><text class="followed"><text class="follow-title">关注</text><text class="num">{{userMine.follows}}</text></text><text class="followed"><text class="follow-title">粉丝</text><text class="num">{{userMine.fans}}</text></text></text></view></view><view class="user2"><view class="user-tags"><block wx:for="{{userMine.tags}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="tag-background"><view class="tag-item"><text class="tag-text">{{item}}</text></view></view></block><view class="discounts"><text data-event-opts="{{[['tap',[['toDiscounts',['$event']]]]]}}" style="margin-right:10rpx;" bindtap="__e">优惠中心</text><text class="icon-rightarrow-white"></text></view></view></view></view><view class="areo-body-box"><view class="areo-body"><view class="title"><view class="title-bar"></view><text class="title-name">我的买卖</text></view><view class="collection-block"><view data-event-opts="{{[['tap',[['toOrder',[0]]]]]}}" class="collection-item" bindtap="__e"><text class="category">市集</text><text class="iconfont icon-rightarrow" style="float:right;"></text><text class="collection-count">{{"共"+ordergoodsNum+"个"}}</text></view><view data-event-opts="{{[['tap',[['toOrder',[1]]]]]}}" class="collection-item" bindtap="__e"><text class="category">拍卖</text><text class="iconfont icon-rightarrow" style="float:right;"></text><text class="collection-count">{{"共"+orderauctionsNum+"个"}}</text></view></view><view class="title"><view class="title-bar"></view><text class="title-name">我的收藏</text></view><view class="collection-block"><view data-event-opts="{{[['tap',[['toCollection',[0]]]]]}}" class="collection-item" bindtap="__e"><text class="category">商品</text><text class="iconfont icon-rightarrow" style="float:right;"></text><text class="collection-count">{{"共"+collgoodsNum+"个"}}</text></view><view data-event-opts="{{[['tap',[['toCollection',[1]]]]]}}" class="collection-item" bindtap="__e"><text class="category">拍卖品</text><text class="iconfont icon-rightarrow" style="float:right;"></text><text class="collection-count">{{"共"+collauctionsNum+"个"}}</text></view></view><view class="title"><view class="title-bar"></view><text class="title-name">历史踪迹</text><text data-event-opts="{{[['tap',[['toMoreHistory',['$event']]]]]}}" class="more-history" bindtap="__e">查看更多</text></view><view class="history-block"><block wx:for="{{userHistoryLate}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toClassify',['$0','$1'],[[['userHistoryLate','',index,'id']],[['userHistoryLate','',index,'type']]]]]]]}}" class="history-item" bindtap="__e"><view class="pic-box"><image class="pic" src="{{item.introImage[0]}}" lazy-load="true" mode="aspectFill"></image></view></view></block></view></view><view class="areo-bottom">......已经到底啦......</view></view></view>