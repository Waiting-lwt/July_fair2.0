.paimai-nav {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 56rpx 0;
  width: 100%;
  height: 46rpx;
  /* background-color: #4CD964; */
}
.paimai-nav .paimai-nav-left {
  width: 10rpx;
  height: 24rpx;
  background-color: #21277b;
}
.paimai-nav .paimai-nav-jh,
.paimai-nav .paimai-nav-px,
.paimai-nav .paimai-nav-fl {
  display: flex;
  position: relative;
  height: 46rpx;
  line-height: 46rpx;
  text-align: center;
  width: 82rpx;
  color: #4f73a5;
  margin-left: 10rpx;
  border-radius: 10rpx;
}
.paimai-nav .paimai-nav-jh text,
.paimai-nav .paimai-nav-px text,
.paimai-nav .paimai-nav-fl text {
  font-size: 24rpx;
  padding-left: 14rpx;
}
.bg1 {
  background-color: #e6f0fa;
}
.bg2 {
  background-color: #e6f0fa;
}
.bg3 {
  background-color: #e6f0fa;
}
.paimai-nav > text {
  color: #4f73a5;
  font-weight: 400;
  padding-left: 10rpx;
  font-size: 28rpx;
  line-height: 46rpx;
}
.paimai-nav-null {
  flex: 1;
}
.paimai-list {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.paimai-list .goodslist {
  position: relative;
}
.paimai-list .goodslist .goodsmask {
  display: none;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: #e5e5e5;
  opacity: 0.5;
  z-index: 99;
}
.paimai-list .goodslist .goodsmask .fv {
  position: absolute;
  top: 16rpx;
  right: 24rpx;
  width: 32rpx;
  height: 32rpx;
}
.paimai-list .goodslist .goodsmask .fv::before {
  content: '\e6ab';
  font-family: 'iconfont';
}
.paimai-list .goodslist .goodsmask .maskcontainer {
  position: absolute;
  left: 0;
  bottom: 0;
  height: 30%;
  width: 100%;
  z-index: 990;
  background-image: linear-gradient(#c7c7c7, #2a2a2a, #000000);
}
.paimai-list .goodslist .goodsmask .maskcontainer text {
  position: absolute;
  top: 20rpx;
  left: 40%;
  -webkit-transform: translateX(-34%);
          transform: translateX(-34%);
  font-size: 24rpx;
  color: #ffffff;
  z-index: 999;
}
.paimai-list .goodslist .goodsmask .maskcontainer .keyword {
  color: #FFFFFF;
  width: 94%;
  height: 100%;
  padding: 3px 3%;
  display: flex;
  justify-content: center;
}
.paimai-list .goodslist .goodsmask .maskcontainer .keyword > view {
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 10rpx;
  padding: 3rpx 10rpx;
  margin: 54rpx 20rpx 2rpx 0;
  height: 14rpx;
  font-size: 18rpx;
  background-color: #f2f2f2;
  color: #6b6b6b;
}
.paimai-list > view {
  width: 300rpx;
  height: 298rpx;
  margin-bottom: 25rpx;
  border-radius: 28rpx;
  overflow: hidden;
  box-shadow: -2rpx 2rpx 6rpx 2rpx rgba(0, 0, 0, 0.3);
}
.paimai-list > view image {
  width: 100%;
  height: 100%;
}
.footer {
  margin: 10rpx auto;
  color: #0c8ee5;
  text-align: center;
}
.goodsmask_hidden .goodsmask {
  display: block !important;
}

