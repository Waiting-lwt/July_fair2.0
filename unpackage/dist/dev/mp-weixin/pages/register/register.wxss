.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  margin: 0 48rpx;
}
.container .top {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  height: 176rpx;
  width: 100%;
}
.container .top image {
  height: 30rpx;
  width: 25rpx;
  margin-right: 20rpx;
}
.container .top > text {
  color: #006AD7;
  font-size: 36rpx;
  vertical-align: middle;
}
.container .portrait {
  position: relative;
  height: 170rpx;
  width: 170rpx;
  border-radius: 26rpx;
  margin-bottom: 56rpx;
}
.container .portrait image {
  height: 100%;
  width: 100%;
  border-radius: 26rpx;
}
.container .portrait .add-portrait {
  position: absolute;
  top: 128rpx;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  height: 60rpx;
  width: 60rpx;
  background-color: #006AD7;
  border-radius: 50%;
}
.container .portrait .add-portrait image {
  position: absolute;
  top: 50%;
  left: 50%;
  height: 28rpx;
  width: 28rpx;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
}
.container .input {
  display: flex;
  align-items: center;
  width: 654rpx;
  height: 80rpx;
  border-radius: 24rpx;
  border: 1px solid #8E8E8E;
  box-shadow: 3rpx 2rpx 9rpx #8E8E8E;
  margin-bottom: 20rpx;
}
.container .input > text {
  color: #4F73A5;
  font-size: 30rpx;
  margin: 0 40rpx;
  font-weight: 700;
}
.container .input input {
  margin-left: 20rpx;
  font-size: 24rpx;
}
.container .info {
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  margin: 44rpx 0;
}
.container .info text:nth-child(1) {
  font-size: 24rpx;
  color: #4E73A5;
  margin-bottom: 10rpx;
}
.container .info text:nth-child(2) {
  font-size: 18rpx;
  color: #006AD7;
}
.container .select-btn {
  display: flex;
  justify-content: space-between;
  height: 68rpx;
  width: 100%;
}
.container .select-btn view {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 100%;
  width: 200rpx;
  border: 2rpx solid #006AD7;
  border-radius: 25rpx;
}
.container .select-btn view text {
  font-size: 24rpx;
  color: #006AD7;
}
.container .select-btn .btn11,
.container .select-btn .btn22,
.container .select-btn .btn33 {
  background-color: #006AD7;
}
.container .select-btn .btn11 text,
.container .select-btn .btn22 text,
.container .select-btn .btn33 text {
  color: #ffffff;
}
.container .prompt {
  display: flex;
  align-items: center;
  margin: 40rpx 0 20rpx;
  height: 40rpx;
}
.container .prompt image {
  height: 40rpx;
  width: 40rpx;
  border-radius: 50%;
}
.container .prompt text {
  font-size: 18rpx;
  color: #006AD7;
}
.container .input2 {
  position: relative;
  height: 448rpx;
  border-radius: 24rpx;
  border: 1px solid #8E8E8E;
  box-shadow: 3rpx 2rpx 9rpx #8E8E8E;
  margin-bottom: 160rpx;
}
.container .input2 .input2-top {
  display: flex;
  align-items: center;
  width: 654rpx;
  height: 80rpx;
}
.container .input2 .input2-top > text:nth-child(1) {
  color: #4F73A5;
  font-size: 30rpx;
  margin: 0 40rpx;
  font-weight: 700;
}
.container .input2 .icon-add {
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
  border: 2px solid #DFDDD9;
  border-radius: 10rpx;
  height: 26rpx;
  width: 26rpx;
}
.container .input2 .icon-add > text {
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-51%, -54%);
          transform: translate(-51%, -54%);
  font-weight: 700;
  font-size: 30rpx;
  color: #DFDDD9;
}
.container .input2 .card-img,
.container .input2 .merchant-img {
  height: 320rpx;
  width: 600rpx;
  margin-left: 30rpx;
  border-radius: 30rpx;
  overflow: hidden;
}
.container .input2 .card-img > image,
.container .input2 .merchant-img > image {
  height: 100%;
  width: 100%;
}
.container .block-yk {
  margin: 348rpx 0;
  font-size: 18rpx;
  color: #006AD7;
}
.container .confirmPrice {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
  width: 600rpx;
  height: 342rpx;
  background-color: #ffffff;
  border-radius: 34rpx;
  box-sizing: border-box;
}
.container .confirmPrice > text {
  padding: 40rpx 0 0 10rpx;
  color: #0069D7;
}
.container .confirmPrice > text:nth-child(1) {
  font-size: 28rpx;
  margin-bottom: -60rpx;
}
.container .confirmPrice > text:nth-child(2) {
  font-size: 18rpx;
  margin-top: -20rpx;
}
.container .confirmPrice .confirmPrice-btn {
  display: flex;
  justify-content: space-between;
  width: 468rpx;
  padding-bottom: 40rpx;
}
.container .confirmPrice .confirmPrice-btn .confirm-btn {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 162rpx;
  height: 62rpx;
  background-color: #006AD7;
  border-radius: 20rpx;
}
.container .confirmPrice .confirmPrice-btn .confirm-btn > text {
  color: #ffffff;
  font-size: 20rpx;
}
.confirm {
  position: fixed;
  bottom: 0;
  left: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  height: 104rpx;
  width: 100%;
  background-color: #006AD7;
  z-index: 9;
}
.confirm > text {
  color: #FFFFFF;
  font-size: 36rpx;
}

