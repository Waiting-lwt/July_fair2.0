.container {
  width: 600rpx;
  margin: 50rpx auto 0 auto;
}
.container .good-block {
  height: 250rpx;
  border-bottom: 1rpx solid #000000;
}
.container .good-block .good-image-block {
  float: left;
  width: 200rpx;
  height: 200rpx;
  box-sizing: border-box;
}
.container .good-block .good-image-block image {
  width: 100%;
  height: 100%;
}
.container .good-block .good-info-block {
  box-sizing: border-box;
  float: left;
  width: 400rpx;
  height: 100%;
  padding: 10rpx 0 0 30rpx;
}
.container .good-block .good-info-block .seller-info-block image {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50rpx;
  border: 1rpx solid #000000;
}
.container .good-block .good-info-block .seller-info-block text {
  font-size: 24rpx;
  padding-left: 10rpx;
  vertical-align: 50%;
}
.container .good-block .good-info-block .good-info-name {
  font-size: 32rpx;
  line-height: 35rpx;
  height: 105rpx;
  overflow: hidden;
  text-overflow: ellipsis;
}
.container .logitics-block {
  padding: 30rpx 0 20rpx 0 ;
}
.container .logitics-block .logitcs-item1 {
  float: left;
  width: 160rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.container .logitics-block .logitcs-item1 text {
  height: 70rpx;
  line-height: 70rpx;
  font-size: 30rpx;
}
.container .logitics-block .logitcs-item2 {
  float: left;
  width: 390rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.container .logitics-block .logitcs-item2 text {
  height: 70rpx;
  line-height: 70rpx;
  font-size: 30rpx;
}
.container .logitics-block .logitcs-item3 {
  float: left;
  width: 50rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.container .logitics-block .logitcs-item3 text {
  height: 70rpx;
  line-height: 70rpx;
  font-size: 24rpx;
}

